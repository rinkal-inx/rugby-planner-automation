<suite name="ParallelBrowsers" parallel="tests" thread-count="2">

    <listeners>
        <listener class-name="com.rugby.planner.listeners.TestListener"/>
    </listeners>

    <test name="ChromeTest">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.rugby.planner.tests.LogoutTest"/>
        </classes>
    </test>

    <test name="EdgeTest">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="com.rugby.planner.tests.LogoutTest"/>
        </classes>
    </test>

</suite>
